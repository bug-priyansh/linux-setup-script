        "vscode-kubernetes.kubectl-path-linux": "/home/iampriyansh/.local/state/vs-kubernetes/tools/kubectl/kubectl"
    },
    "snyk.yesWelcomeNotification": false,
    "RainbowBrackets.depreciation-notice": false,
